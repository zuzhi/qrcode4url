{
    "name": "qrcode4url",
    "version": "0.1",
    "description": "QR code generator for the URL",
    "permissions": ["storage", "declarativeContent", "activeTab"],
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "page_action": {
        "default_popup": "components/popup/popup.html",
        "default_icon": {
            "16": "assets/images/get_started16.png",
            "32": "assets/images/get_started32.png",
            "48": "assets/images/get_started48.png",
            "128": "assets/images/get_started128.png"
        }
    },
    "options_page": "components/options/options.html",
    "icons": {
        "16": "assets/images/get_started16.png",
        "32": "assets/images/get_started32.png",
        "48": "assets/images/get_started48.png",
        "128": "assets/images/get_started128.png"
    },
    "manifest_version": 2
}